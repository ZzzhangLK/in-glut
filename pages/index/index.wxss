.Modal_background{
	display: none;
	position:fixed;
	top: 0%;
	left: 0%;
	width: 100%;
	height: 100%;
	background-color: black;
	z-index: 1001;
	-moz-opacity: 0.7;
	opacity: 0.70;
	filter: alpha(opacity=70);
}
.Modal_box{
	display: none;
	text-align: center;
	position:fixed;
	margin: auto;
	top: 0; left: 0; bottom: 0; right: 0;
	width:76%;
	height:50%;
	background-color: white;
	z-index: 1002;
	overflow: auto;
	border-radius:5px;
  -webkit-animation-name: scaleDraw; /*关键帧名称*/
  -webkit-animation-timing-function: ease-in-out; /*动画的速度曲线*/
  -webkit-animation-iteration-count:1;  /*动画播放的次数*/
  -webkit-animation-duration:0.5s; /*动画所花费的时间*/
}
.Modal_bottom{
	height:50px;
	width:100%;
	position:absolute;
	color:#3f51b5;
	font-size:15px;
	bottom:0px;
	left:0px;
}
.Modal_buttom{
	width:50%;
	height:50px;
	line-height:50px;
	text-align:center;
	display:inline-block;
	position:relative;
  /* border:1px solid #cccccc; */
}
.Modal_no{
  color:#808080!important;
}
.Modal_img{
	background-size:100% 100%;
	width:95%;
	position:relative;
	top:0px;
	/* background-image:url(https://yiban.glut.edu.cn/zlk/image/wxapp/modal/抖音大赛.png); */
	height:86%;
	margin:0 auto;
}
@keyframes scaleDraw {  /*定义关键帧、scaleDrew是需要绑定到选择器的关键帧名称*/
    0%{
        transform: scale(0.25);  /*开始为原始大小*/
    }
    75%{
        transform: scale(1);
    }
  }